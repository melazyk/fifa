{
  "name": "Headers Sender",
  "version": "1.0",
  "description": "Send headers form tab to anohter url",
  "action": {
    "default_icon": "images/red.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 3
}